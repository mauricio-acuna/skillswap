# SkillSwap Frontend - Environment Variables Template
# Copy this file to .env and fill in your actual values

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8080/api/v1
REACT_APP_WEBSOCKET_URL=ws://localhost:8080

# External Services
REACT_APP_AGORA_APP_ID=your_agora_app_id_here
REACT_APP_GOOGLE_CALENDAR_CLIENT_ID=your_google_client_id_here
REACT_APP_FCM_SENDER_ID=your_fcm_sender_id_here

# Deep Linking
REACT_APP_DEEP_LINK_URL=skillswap://dev

# Development Settings
REACT_APP_ENVIRONMENT=development
REACT_APP_API_TIMEOUT=10000
REACT_APP_CACHE_DURATION=300000
REACT_APP_DEBUG_MODE=true

# File Upload Settings
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_SUPPORTED_VIDEO_FORMATS=mp4,avi,mov
REACT_APP_SUPPORTED_IMAGE_FORMATS=jpg,jpeg,png,webp

# Analytics & Monitoring (Optional)
REACT_APP_ANALYTICS_KEY=your_analytics_key_here
REACT_APP_CRASH_REPORTING_KEY=your_crash_reporting_key_here
